nbVow = str_count(tolower(Prenom), "[aeoiuy]"),
nbCon = str_count(tolower(Prenom), "[^aeoiuy]"))
MoyenneConAndVow <- ddply(prenomsParisC,"Annee",summarize,MeanCon=mean(nbCon),MeanVow=mean(nbVow))
specify_decimal <- function(x, k) trimws(format(round(x, k), nsmall=k))
ggplot() +
geom_line(data=MoyenneConAndVow, aes(x=Annee, y=MeanCon, colour="red"), size = 2) +
geom_line(data=MoyenneConAndVow, aes(x=Annee, y=MeanVow, colour="blue"), size = 2) +
scale_color_discrete(name = "Type de lettre", labels = c("Voyelle", "Consonne")) +
labs(x="Annee", y="Nombre moyen d'occurence par type de lettre")
